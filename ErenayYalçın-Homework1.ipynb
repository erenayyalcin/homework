    "#denklem : ax**2 + b*x + c\n",
    "\n",
    "a=int(input(\"a değeri:\"))\n",
    "b=int(input(\"b değeri:\"))      \n",
    "c=int(input(\"c değeri:\"))\n",
    "\n",
    "delta = b ** 2 - 4*a*c\n",
    "\n",
    "x1 = (-b + delta ** 0.5)/2*a\n",
    "x2 = (-b - delta ** 0.5)/2*a\n",
    "\n",
    "print(\"x1:{}\\n  x2:{}\\n\" .format(x1,x2))     "
   ]
  }
